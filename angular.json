{
    "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
    "version": 1,
    "newProjectRoot": "projects",
    "projects": {
        "ngx-gallery": {
            "projectType": "application",
            "schematics": {
                "@schematics/angular:component": {
                    "style": "scss"
                }
            },
            "root": "projects/",
            "sourceRoot": "projects/ngx-gallery/src",
            "prefix": "app",
            "architect": {
                "build": {
                    "builder": "@angular-devkit/build-angular:browser",
                    "options": {
                        "outputPath": "dist/ngx-gallery",
                        "index": "projects/ngx-gallery/src/index.html",
                        "main": "projects/ngx-gallery/src/main.ts",
                        "polyfills": "projects/ngx-gallery/src/polyfills.ts",
                        "tsConfig": "projects/ngx-gallery/tsconfig.app.json",
                        "aot": true,
                        "assets": [
                            "projects/ngx-gallery/src/favicon.ico",
                            "projects/ngx-gallery/src/assets"
                        ],
                        "styles": ["projects/ngx-gallery/src/styles.scss"],
                        "scripts": []
                    },
                    "configurations": {
                        "production": {
                            "fileReplacements": [
                                {
                                    "replace": "projects/ngx-gallery/src/environments/environment.ts",
                                    "with": "projects/ngx-gallery/src/environments/environment.prod.ts"
                                }
                            ],
                            "optimization": true,
                            "outputHashing": "all",
                            "sourceMap": false,
                            "extractCss": true,
                            "namedChunks": false,
                            "extractLicenses": true,
                            "vendorChunk": false,
                            "buildOptimizer": true,
                            "budgets": [
                                {
                                    "type": "initial",
                                    "maximumWarning": "2mb",
                                    "maximumError": "5mb"
                                },
                                {
                                    "type": "anyComponentStyle",
                                    "maximumWarning": "6kb",
                                    "maximumError": "10kb"
                                }
                            ]
                        }
                    }
                },
                "serve": {
                    "builder": "@angular-devkit/build-angular:dev-server",
                    "options": {
                        "browserTarget": "ngx-gallery:build"
                    },
                    "configurations": {
                        "production": {
                            "browserTarget": "ngx-gallery:build:production"
                        }
                    }
                },
                "extract-i18n": {
                    "builder": "@angular-devkit/build-angular:extract-i18n",
                    "options": {
                        "browserTarget": "ngx-gallery:build"
                    }
                },
                "test": {
                    "builder": "@angular-devkit/build-angular:karma",
                    "options": {
                        "main": "projects/ngx-gallery/src/test.ts",
                        "polyfills": "projects/ngx-gallery/src/polyfills.ts",
                        "tsConfig": "projects/ngx-gallery/tsconfig.spec.json",
                        "karmaConfig": "projects/ngx-gallery/karma.conf.js",
                        "assets": [
                            "projects/ngx-gallery/src/favicon.ico",
                            "projects/ngx-gallery/src/assets"
                        ],
                        "styles": ["projects/ngx-gallery/src/styles.scss"],
                        "scripts": []
                    }
                },
                "lint": {
                    "builder": "@angular-devkit/build-angular:tslint",
                    "options": {
                        "tsConfig": [
                            "projects/ngx-gallery/tsconfig.app.json",
                            "projects/ngx-gallery/tsconfig.spec.json",
                            "projects/ngx-gallery/e2e/tsconfig.json"
                        ],
                        "exclude": ["**/node_modules/**"]
                    }
                },
                "e2e": {
                    "builder": "@angular-devkit/build-angular:protractor",
                    "options": {
                        "protractorConfig": "projects/ngx-gallery/e2e/protractor.conf.js",
                        "devServerTarget": "ngx-gallery:serve"
                    },
                    "configurations": {
                        "production": {
                            "devServerTarget": "ngx-gallery:serve:production"
                        }
                    }
                }
            }
        }
    },
    "cli": {
        "analytics": false
    },
    "defaultProject": "ngx-gallery"
}
